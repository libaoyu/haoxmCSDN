		/*背景*/
		.wrapper:before {
		  content: ' ';
		  position: fixed;
		  z-index: -1;
		  top: 0;
		  right: 0;
		  bottom: 0;
		  left: 0;
		 
		  background-size: cover;
		}
		.main_wrapper {
			background-attachment: fixed;
			background-size: 100% 100%;
			background-position: center center;
			overflow: hidden;
			
		}
		
		
		@media screen and (max-width:768px){
			.main_wrapper{
				padding-top: 170%;
			}
		}
		@media screen and (min-width:768px) and (max-width:992px){
			.main_wrapper{
				padding-top: 125%;
			}
		}
		@media screen and (min-width:992px) and (max-width:1200px){
			.main_wrapper{
				padding-top: 125%;
			}
		}
		@media screen and (min-width:1211px) {
			.main_wrapper{
				padding-top: 30%;
			}
		}
		/*内容边框*/
		.survey_main{
			position: relative;
		}
		.main_content { 
			/*margin-top: 80%;*/
			background-color: #fff;
			width: 96%;
			margin: 10px auto;
			padding: 5px;
			
		}
		.main_border{
			border: 2px solid #fbdade;
		}
		.main_content::before{
			content: "";
			width: 15px;
			height: 12px;
			-webkit-transform: rotate(-50deg);
			transform: rotate(-50deg);
			left: 8px;
			top: 2px;
			position: absolute;
			border-bottom: 2px solid #fbdade;
			border-radius: 50%;
			background-color: #fff;
		}
		.main_content::after{
			content: "";
			width: 15px;
			height: 12px;
			-webkit-transform: rotate(36deg);
			transform: rotate(36deg);
			right: 9px;
			top: 3px;
			position: absolute;
			border-bottom: 2px solid #fbdade;
			border-radius: 50%;
			background-color: #fff;
		}
		.border_corner::after{
			content: "";
			width: 15px;
			height: 12px;
			-webkit-transform: rotate(36deg);
			transform: rotate(136deg);
			right: 9px;
			bottom: 3px;
			position: absolute;
			border-bottom: 2px solid #fbdade;
			border-radius: 50%;
			background-color: #fff;
		}
		.border_corner::before{
			content: "";
			width: 15px;
			height: 12px;
			-webkit-transform: rotate(36deg);
			transform: rotate(-120deg);
			left: 9px;
			bottom: 3px;
			position: absolute;
			border-bottom: 2px solid #fbdade;
			border-radius: 50%;
			background-color: #fff;
		}
		@media screen and (max-width:992px) and (min-width:768px){
			.main_content::before{
				content: "";
				top:3px;
				left: 17px;
			}
			.main_content::after{
				content: "";
				top:3px;
				right: 17px;
			}
			.border_corner::after{
				content: "";
				right: 17px;
				bottom: 3px;
			}
			.border_corner::before{
				content: "";
				left: 17px;
				bottom: 3px;
			}
		}
		@media screen and (max-width:1200px) and (min-width:980px){
			.main_content::before{
				content: "";
				top:3px;
				left: 22px;
			}
			.main_content::after{
				content: "";
				top:3px;
				right: 22px;
			}
			.border_corner::after{
				content: "";
				right: 22px;
				bottom: 3px;
			}
			.border_corner::before{
				content: "";
				left: 22px;
				bottom: 3px;
			}
		}
		@media (min-width:1200px){
			.main_content::before{
				content: "";
				top:4px;
				left: 28px;
			}
			.main_content::after{
				content: "";
				top:4px;
				right: 28px;
			}
			.border_corner::after{
				content: "";
				right: 28px;
				bottom: 4px;
			}
			.border_corner::before{
				content: "";
				left: 28px;
				bottom: 4px;
			}
		}
