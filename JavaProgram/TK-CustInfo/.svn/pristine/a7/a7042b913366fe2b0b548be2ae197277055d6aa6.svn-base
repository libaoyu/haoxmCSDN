}

.loading{  
		    width:220px;  
		    height:56px;  
		    position: fixed;  
		    top:50%;  
		    left:50%;  
		    transform: translateX(-50%);
            -webkit-transform:translateX(-50%);
		    line-height:56px;  
		    color:#fff;  
		    padding-left:80px;  
		    font-size:15px;  
		    background: #000 url(../../static/img/loader.gif) no-repeat 20px 50%;  
		    opacity: 0.7;  
		    z-index:9999;  
		    -moz-border-radius:20px;  
		    -webkit-border-radius:20px;  
		    border-radius:20px;  
		    filter:progid:DXImageTransform.Microsoft.Alpha(opacity=70);  
		    display:none;
		}  

