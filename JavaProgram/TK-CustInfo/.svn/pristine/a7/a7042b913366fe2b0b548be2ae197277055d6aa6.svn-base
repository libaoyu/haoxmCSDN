/* reset */
body,h1,h2,h3,h4,h5,h6,p,dl,dd,ul,ol,pre,form,input,textarea,th,td,select{margin:0;padding:0;}
body,input{	
	font-size:0.16rem; color: #363636;
}
body,h1,h2,h3,h4,h5,h6,p,dl,dd,ul,ol,th,td,pre,form,button,input,strong,span,em,select,textarea{	
	font-family:'PingFangSC','helvetica neue','hiragino sans gb','arial','microsoft yahei ui','microsoft yahei','simsun','sans-serif' !important;
}
html,body{height: 100%;width:100% ;}
em{font-style:normal}
li{list-style:none}
a{text-decoration:none; -webkit-tap-highlight-color: transparent;}
img{border:none; vertical-align:top;}
table{border-collapse:collapse;}
textarea{ resize:none;overflow:auto;text-align:left;font-size:0.16rem;}
.clear{zoom:1;}
.clear:after{ content:''; display:block; clear:both;}
input{font-size:0.14rem}
/*此为基础样式*/
html{
   	font-size:100px;	
}

.clc{
    clear: both;
    overflow: auto;
}
.topNav{
	width: 100%;
	height: 48px;
	text-align: center;
	line-height: 48px;
	color: #fff;
	background: #00cc99;
	font-size: 0.18rem;
	}
.contentAll{
	width: 100%;
	height: 100%;
	position: relative;
}
.titleLine{
	height:0.17rem;
	width: 4px;
	margin-right:12px;
	background: #00cc99;
	display: inline-block;
}
.titleC{
	font-size: 0.17rem;
	float: left;
	padding:12px 7px 7px 14px ;
	box-sizing: border-box;
	line-height: 0.17rem;
	font-weight:700;
}
.doctorInfor{
	box-sizing: border-box;

}
.doctorInforContent{
	width: 100%;
	height: auto;
	border: solid 1px #cfcfcf;
	border-radius: 8px;
	position: relative;
	padding-bottom:10px;
}
.doctorImg{
	display: inline-block;
	width:80px;
	height:80px;
	position: absolute;
	top:50%;
	left:15px;
	vertical-align:middle;
	margin-top:-40px;
}
.doctorImg img{
	width: 80px;
	height: 80px;
	border-radius: 50%;

}
.doctorRightInfor{
	box-sizing: border-box;
	width: 100%;
	height: auto;
	padding-left: 112px;
}
.doctorRightInfor p{
	font-size: 0.15rem;
	line-height: 30px;
}
.doctorRightInfor p:nth-child(1){
	margin-top:8.5px
}
.doctorRightInfor span{
	color: #898989;
}
.doctorName{
	color: #ff9900 !important;
}
